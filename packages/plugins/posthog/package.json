{
    "name": "@onboardjs/posthog-plugin",
    "version": "1.0.3",
    "private": false,
    "description": "Official PostHog analytics plugin for OnboardJS.",
    "files": [
        "dist"
    ],
    "main": "dist/index.cjs.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.es.js",
            "require": "./dist/index.cjs.js"
        }
    },
    "scripts": {
        "build": "vite build && tsc --emitDeclarationOnly true",
        "test": "vitest --coverage"
    },
    "keywords": [
        "onboardjs",
        "posthog",
        "analytics",
        "onboarding",
        "tracking",
        "plugin"
    ],
    "author": {
        "name": "Soma Somorjai",
        "email": "soma@onboardjs.com",
        "url": "https://somafet.com"
    },
    "license": "MIT",
    "homepage": "https://onboardjs.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/Somafet/onboardjs.git",
        "directory": "packages/plugins/posthog"
    },
    "peerDependencies": {
        "@onboardjs/core": ">=0.3.12",
        "posthog-js": ">=1.255.1",
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0"
    },
    "devDependencies": {
        "@onboardjs/core": "workspace:*",
        "@types/node": "^24.3.0",
        "@types/react": "^19.1.12",
        "@types/react-dom": "^19.1.9",
        "@vitest/coverage-v8": "^3.1.4",
        "posthog-js": "^1.255.1",
        "typescript": "^5.8.3",
        "vitest": "^3.1.4"
    }
}
