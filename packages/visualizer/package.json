{
  "name": "@onboardjs/visualizer",
  "version": "0.1.3",
  "description": "A React component library to visually build and edit OnboardJS flows.",
  "author": "Soma Somorjai <soma@onboardjs.com>",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/src/index.d.ts",
  "homepage": "https://onboardjs.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Somafet/onboardjs.git",
    "directory": "packages/visualizer"
  },
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./index.css": "./dist/index.css"
  },
  "scripts": {
    "dev": "vite",
    "start": "cd dist && npx serve",
    "build": "vite build && tsc --emitDeclarationOnly true"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@onboardjs/core": "*",
    "@tailwindcss/vite": "^4.1.11",
    "@xyflow/react": "^12.8.2",
    "dagre": "^0.8.5",
    "install": "^0.13.0",
    "lucide-react": "^0.525.0",
    "npm": "^11.4.2",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@types/dagre": "^0.7.53",
    "@types/node": "^22.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
